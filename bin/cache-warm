#!/bin/sh

#
# Warm cache for build and test processes
#

DOCKER=$(which docker)

if [ DOCKER = '' ]; then
    echo 'Docker is required\n'
    exit 1
fi

echo "#\n# Warming cache\n#\n"
for image in "httpd:alpine docker.io/alphasocket/php-fpm-alpine:7.1"
do 
    docker pull ${image} 1>/dev/null;
done

exit 0